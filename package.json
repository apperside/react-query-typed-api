{
  "name": "react-query-typed-api",
  "version": "0.0.149",
  "description": "An opinionated wrapper around react-query for strongly typed api requests",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "chromatic": "chromatic --exit-zero-on-changes",
    "lint": "eslint --fix 'src/**/*.{js,ts,tsx}'",
    "prepare": "husky install",
    "release": "npm run build && npm version patch && npm publish",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apperside/react-query-typed-api.git"
  },
  "keywords": [
    "react",
    "library",
    "storybook",
    "components"
  ],
  "author": "Apperside",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apperside/react-query-typed-api/issues"
  },
  "homepage": "https://github.com/apperside/react-query-typed-api#readme",
